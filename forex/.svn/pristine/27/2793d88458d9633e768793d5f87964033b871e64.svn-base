@charset "utf-8";
/* CSS Document */
/*初始化*/
body	{padding:0;	margin:0 auto; font-family:"宋体", "黑体", sans-serif; font-size:14px;text-align:left;background:#fff}
div, h1, h2, h3, h4, p, form, label, input, textarea, img, span,ul,li{margin:0; padding:0;}
h1, h2, h3, h4{ font-size:14px; font-weight:normal;}
h1, h2 { font-size:14px;}
ul	{margin:0; padding:0; list-style-type:none;}
/*ul	{margin:0; padding:0; list-style-type:none;	font-size:0;}*/
.clear{	clear:both; line-height:0px; height:0px; text-decoration:none;	}
.more { float:right; margin-top:-25px; margin-right:2px;}
.more2 { float:right; margin-top:-30px; margin-right:2px;}
.textcolor{ color:#003366}

a,a:visited{ color:Black; text-decoration:none;border:none }
a:hover{ color:#073357; text-decoration:underline;}
a img { border:none; text-decoration:none; }
a:visited img{ border:none; text-decoration:none; }
a:hover img{ border:none; text-decoration:none; }
img{border:none;}


#container{margin:0 auto; width:960px;line-height:1.5em;text-align:left;}
#container2{ width:1000px; margin:0 auto; line-height:1.5em;background:#FFFFFF;text-align:left;}

/*登陆模块*/
#login{ width:100%; height:317px; background:url(../image/newImages/bgLogin3.jpg) no-repeat; margin-top:80px; }
#login h1{ margin-left:190px; margin-top:15px;}
.formLogin{ margin-top:50px; padding-left:380px;}
.formLogin table td{ padding:5px 20px 5px 0px}
#copyright{ display:block; margin-top:60px; padding:0 0 0 560px}
.btn0{background:url(../image/newImages/btnBg0.jpg) no-repeat; width:118px; height:30px; border:none; }
.btn{background:url(../image/newImages/btnBg.jpg) no-repeat; width:118px; height:30px; border:none; color:#FFFFFF }
/* zwk begin*/
.formLogin div{margin-bottom: 20px;}
.formLogin div span{font-size:12px;padding:5px 20px 5px 10px;}
/* zwk end */


/*内页模块*/
.nav img{ text-align:center; margin-bottom:7px;} 
#menu{ width:90%; margin:0 auto;}
#top{  background:url(../image/newImages/eesTitlebg.jpg) repeat-x;height:81px; margin-bottom:5px; text-align:left;}
.top_td	{ background:url(../image/newImages/eesTitleRihgt.jpg) repeat-x;padding-top:20px;padding-right:10px;width:617px}


#top h1{ margin-left:20px; margin-bottom:14px; margin-top:15px; }
#top span{ display:block; background-color:#e9f0ff; padding-left:20px}
#left{ float:left; width:19%;}
#right{ float:right;width:80%; margin-right:3px; margin-top:10px;}
.form { }
.form fieldset legend{font-size:14px;border:#1f54bc solid 1px; background:url(../image/newImages/bgLegend.gif) repeat-x #2777b6; padding:2px 10px 1px 10px; font-weight:bold; color:#FFFFFF}
.form fieldset table{ width:100%; font-size:12px; margin-top:15px;}
.form fieldset table td{ padding:4px 0px;}

fieldset legend{font-size:14px;border:#1f54bc solid 1px; background:url(../image/newImages/bgLegend.gif) repeat-x #2777b6; padding:2px 10px 1px 10px; font-weight:bold; color:#FFFFFF}
fieldset table{ width:100%; font-size:12px; margin-top:15px;}
fieldset table td{ padding:4px 0px;}

.attation{ background:#e7eefb; border: solid #CCC 1px; margin-top:5px; color:#900; text-align:left;}
#bottom{ background:url(../image/newImages/bottomBg.gif) repeat-x; text-align:center; height:28px; padding-top:15px;}

/*8.25*/
input,select{
	background: #FFF;
	border: 1px solid #4C77B6;
	color: #000;
	/*ggm 20140223*/
	/*font-family: "Tahoma", "MS Shell Dlg";*/
	font-size: 12PX;
	margin: 1px;
	padding: 1px;
	vertical-align: middle;
}

.button {
	font: 12px Tahoma, Verdana;
	padding: 0 2px;
	color: #000000;
	background-image: url(../image/btnbg.gif);
	background-repeat: repeat-x;
	background-position: 0 50%;
	outline: 1px solid #86B9D6;
	border: 1px solid #FFF !important;
	height: 19px !important;
	border: 1px solid #86B9D6;
	height: 21px;
	line-height: 17px;
	cursor:hand;
}


/*模板管理模块*/
