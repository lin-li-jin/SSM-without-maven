package com.talent.forex.core;

import java.io.Serializable;

/**
 * 系统配置表
 * */
public class Sysconfigs implements Serializable
{
  private Integer id;//主键
  private String ckey;//key
  private String cval;//描述
  private String remark;//注释

  public Sysconfigs()
  {
  }

  public Sysconfigs(Integer id)
  {
    this.id = id;
  }

  public Sysconfigs(Integer id, String ckey, String cval, String remark)
  {
    this.id = id;
    this.ckey = ckey;
    this.cval = cval;
    this.remark = remark;
  }

  public Integer getId()
  {
    return this.id;
  }

  public void setId(Integer id) {
    this.id = id;
  }

  public String getCkey() {
    return this.ckey;
  }

  public void setCkey(String ckey) {
    this.ckey = ckey;
  }

  public String getCval() {
    return this.cval;
  }

  public void setCval(String cval) {
    this.cval = cval;
  }

  public String getRemark() {
    return this.remark;
  }

  public void setRemark(String remark) {
    this.remark = remark;
  }
}
