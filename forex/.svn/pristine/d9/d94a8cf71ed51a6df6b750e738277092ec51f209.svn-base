					/jsp/local/tradeModule/detail/ForwardTradeDetail.jsp
				</param>
			</result>
			<result name="Swap" type="dispatcher">
				<param name="location">
					/jsp/local/tradeModule/detail/swapTradeDetail.jsp
				</param>
			</result>
			<interceptor-ref name="paramsLoginStack"></interceptor-ref>
		</action>
