			<interceptor-ref name="paramsLoginStack"></interceptor-ref>
		</action>
		
		<action name="foreignPageRefresh" method="foreignPageRefresh" class="com.talent.forex.modules.trade.ForeignTradeAction">
			<interceptor-ref name="paramsLoginStack"></interceptor-ref>
		</action>
