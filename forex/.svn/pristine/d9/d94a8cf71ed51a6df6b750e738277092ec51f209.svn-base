				<param name="location">
					/jsp/local/tradeModule/home/CNYHomePage.jsp
				</param>
			</result>
			<result name="oneclick" type="dispatcher">
				<param name="location">
					/jsp/local/tradeModule/home/oneClickHomePage.jsp
				</param>
			</result>
			<interceptor-ref name="paramsLoginStack"></interceptor-ref>
		</action>
