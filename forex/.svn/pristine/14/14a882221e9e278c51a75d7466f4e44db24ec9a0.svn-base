/* CSS Document */
a
{
	text-decoration:none;
}

body
{
	margin:0;
	padding:10px 0 5px 0;
	font:14px tahoma,宋体,sans-serif;
}
*{margin:0;padding:0px}
p a,p a:visited
{
	color:#00f;
	background:inherit;
}

.TreeMenu img.s
{
	cursor:hand;
	vertical-align:middle;
}
.TreeMenu ul
{
	padding:0;
}
.TreeMenu li
{
	list-style:none;
	padding:0;
}
.Closed ul
{
	display:none;
}
.Child img.s
{
	background:none;
	cursor:default;
}

#CategoryTree ul
{
	margin:0 0 0px 0px;
}
