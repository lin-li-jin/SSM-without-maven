/* CSS Document */
a
{
	text-decoration:none;
}

body
{
	margin:0;
	padding:10px 0 5px 0;
	font:14px tahoma,宋体,sans-serif;
}
*{margin:0;padding:0px}
p a,p a:visited
{
	color:#00f;
	background:inherit;
	padding:10px 0 5px 0;
}

.TreeMenu img.s
{
	cursor:hand;
	vertical-align:middle;
	width:150px;
	height:100%;
}
.TreeMenu ul
{
	padding:8px 0 0 0;
}
.TreeMenu li
{
	list-style:none;
	
	padding:0px 0 0px 0;
}
.Closed ul
{
	display:none;
}
.Child img.s
{
	background:none;
	cursor:default;
}

#CategoryTree ul
{
	margin:0 0 0px 0px;
}
