<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.talent.exam.domain.ExamOco" table="exam_oco" schema="forex">
        <id name="examOcoNo">
            <column name="exam_oco_NO" sql-type="int(5)"/>
        </id>
        <property name="direction">
            <column name="DIRECTION" sql-type="char(1)" length="1"/>
        </property>
        <property name="acc">
            <column name="ACC" sql-type="char(3)" length="3"/>
        </property>
        <property name="takeProfitAmount">
            <column name="TAKE_PROFIT_AMOUNT" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="takeProfitPrice">
            <column name="TAKE_PROFIT_PRICE" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="stopLossAmount">
            <column name="STOP_LOSS_AMOUNT" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="stopLossPrice">
            <column name="STOP_LOSS_PRICE" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="monitorPrice">
            <column name="MONITOR_PRICE" sql-type="char(3)" length="3"/>
        </property>
        <property name="goodFrom">
            <column name="GOOD_FROM" sql-type="varchar(8)" length="8"/>
        </property>
        <property name="goodTill">
            <column name="GOOD_TILL" sql-type="varchar(8)" length="8"/>
        </property>
        <property name="step">
            <column name="STEP" sql-type="int(1)"/>
        </property>
        <property name="examScore">
            <column name="exam_score" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="userType">
            <column name="user_type" sql-type="char(1)" length="1"/>
        </property>
        <many-to-one name="examContentByExamNo" class="com.talent.exam.domain.ExamContent">
            <column name="exam_NO"/>
        </many-to-one>
    </class>
</hibernate-mapping>