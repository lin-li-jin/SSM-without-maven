<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.talent.exam.test.ExamAccType" table="exam_acc_type" schema="forex">
        <id name="accTypeNo">
            <column name="acc_type_NO" sql-type="char(1)" length="1"/>
        </id>
        <property name="accType">
            <column name="acc_type" sql-type="varchar(15)" length="15"/>
        </property>
        <set name="examContentsByAccTypeNo" inverse="true">
            <key>
                <column name="acc_type_NO"/>
            </key>
            <one-to-many not-found="ignore" class="com.talent.exam.test.ExamContent"/>
        </set>
    </class>
</hibernate-mapping>