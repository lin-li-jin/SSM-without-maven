package com.hh;
import java.util.ArrayList;
import java.util.Scanner;

public class JavaTest {
	
	///////
	public static void main(String[] args){
	       Scanner sc= new Scanner(System.in);
	        int i=sc.nextInt();
	        if(i<2||i>100){
	        	 i=sc.nextInt();
	        }
	        int a[]=new int [100];
	        for(int x=0;x<i;x++){
	        	a[x]=sc.nextInt();
	        	if(a[x]<2||a[x]<1000){
	        		a[x]=sc.nextInt();
	        	}
	        }
	        for(int y=0;y<i;y++){
	        	for(int z=1;z<i;z++)
	        	a[z+1]=a[z+1]+a[z]/2;
	        	
	        }
	        
	    }
	

	//////////
		
}
