package com.hh;
import java.util.ArrayList;
import java.util.Scanner;

public class JavaTest {
	
	///////
public static void main(String[] args) {
		
	int sum=0;
	Scanner sc = new Scanner(System.in);
	int ww  = sc.nextInt();
//	
	for (int i = 1; i <10; i++) {
		for (int j = 1; j <10; j++) {
			if (i==j) {
				continue;
			}
			for (int z =1; z <10; z++) {
				if (z==i || z==j) {
					continue;
				}
				for (int x = 1; x <10; x++) {
					if (x==i || x==j || x==z) {
						continue;
					}
					for (int c = 1; c <10; c++) {
						if (c==i || c==j || c==z || c==x) {
							continue;
						}
						for (int v = 1; v <10; v++) {
							if (v==i || v==j || v==z || v==x || v==c) {
								continue;
							}
							for (int b = 1; b <10; b++) {
								if (b==i || b==j || b==z || b==x || b==c || b==v) {
									continue;
								}
								for (int n = 1; n <10; n++) {
									if (n==i || n==j || n==z || n==x || n==c || n==v || n==b) {
										continue;
									}
									for (int k = 1; k < 10; k++) {
										if (n==i || n==j || n==z || n==x || n==c || n==v || n==b || k==n) {
											continue;
										}
										sum++;
										
										
										
										
										
										
										
										
										
										
										
										
									}
								}	
							}	
						}		
					}	
				}	
			}	
		}	
	}	
	
	
	
	
	
	System.out.println(sum);

		}

	//////////
		
}
